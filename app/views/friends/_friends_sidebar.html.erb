<h3>People I'm Following</h3>
<p>
<%= link_to "Find", friends_path %> more moms
</p>
<ul class="clearfix">
  <% user.followed_users.each do |followed| -%>
    <li>
      <%= link_to "#" do %>
        <%= image_tag followed.profile_picture.friend, alt: display_name(followed) %>
        <%= display_first_name_or_username(followed) %> <%= display_last_name(followed) %>
      <% end -%>
    </li>
  <% end -%>
</ul>
