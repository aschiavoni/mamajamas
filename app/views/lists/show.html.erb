<%= content_for :title_links do %>
<ul>
  <li>
    <a href="#" id="add-product-type" class="button">
      <span class="ss-plus"></span>
      New Item
    </a>
  </li>
  <li>
  <%= link_to profile_path, id: "bt-share", class: "button" do %>
    <span class="ss-action"></span>Publish
  <% end -%>
  </li>
  <li id="print">
    <a href="#" class="ss-icon" title="Print"><span>Print</span></a>
  </li>
</ul>
<% end -%>
<%= content_for :nav do %>
<nav id="categories">
  <ul>
    <% @categories.each do |category| -%>
      <li class="<%= "cat-selected" if category.id == @category.id %>">
        <%= link_to category.name, list_category_path(category.slug) %>
      </li>
    <% end -%>
  </ul>
</nav>
<% end -%>
<%= content_for :tertiary do %>
<div id="module-follow">
  <div class="content">
    <%= render partial: "friends/friends_sidebar", locals: { user: current_user } %>
  </div>
</div>
<div id="module-msg">
  <div class="content">
    <h3>Reach Out</h3>
    <p>Send a message to ask for advice, or to share it.</p>
  </div>
</div>
<% end -%>
<form class="label-infield" id="my-list" data-list-entries="<%= @list_entries_json %>">
</form>
<script type="text/javascript">
Mamajamas.Context.List = new Mamajamas.Models.List({
  title: '<%= @list.title %>',
  category_id: '<%= @category.id %>',
  category: '<%= @category.slug %>',
  current_position: 0
})
</script>
