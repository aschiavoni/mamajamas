<%= content_for :title_links do %>
  <ul>
  <li id="bt-follow"><a href="#" class="button">Follow</a></li>
  <li id="print"><a href="#" class="ss-icon" title="Print"><span>Print</span></a></li>
  <li id="pinterest"><a href="#" class="social" title="Pinterest">Pinterest</a></li>
  <li id="twitter"><a href="#" class="social" title="Twitter">Twitter</a></li>
  <li id="facebook"><a href="#" class="social" title="Facebook">Facebook</a></li>
</ul>
<% end -%>
<%= content_for :nav do %>
  <nav id="categories">
    <ul>
      <% @categories.each do |category| -%>
        <li class="<%= "cat-selected" if category.id == @category.id %>">
          <%= link_to category.name,
            public_list_category_path(current_user.username, category.slug) %>
        </li>
      <% end -%>
    </ul>
  </nav>
<% end -%>
